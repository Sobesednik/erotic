{
  "name": "erotic",
  "version": "1.2.1",
  "description": "Capture synchronous part of the error stack in asynchronous functions",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "build": "alamode src -o build",
    "e": "node example/run",
    "example/message.js": "yarn e example/message",
    "example/error.js": "node example/run example/error",
    "example/transparent.js": "node example/run example/transparent",
    "example/read-file.js": "node example/run example/read-file",
    "example/erotic.js": "node example/run example/erotic"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/erotic.git"
  },
  "keywords": [
    "error",
    "stack",
    "throw",
    "preserve",
    "line",
    "column",
    "internal",
    "node",
    "reject"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/erotic/issues"
  },
  "homepage": "https://github.com/artdecocode/erotic#readme",
  "devDependencies": {
    "eslint-config-artdeco": "1.0.0",
    "noddy": "1.1.1",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  },
  "dependencies": {
    "clean-stack": "1.3.0"
  }
}
