{
  "name": "erotic",
  "version": "1.1.0",
  "description": "Capture synchronous part of the error stack in asynchronous functions",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "zoroaster test/spec -b",
    "test-build": "BABEL_ENV=test-build yarn t test/spec",
    "build-test": "BABEL_ENV=test-build babel test --out-dir test-build",
    "@test-build": "zoroaster test-build/spec",
    "test-es5": "LIB_MAIN=../../es5 zoroaster test/spec -b",
    "test-watch": "zoroaster test/spec -w",
    "test-all": "yarn-s test test-build test-es5",
    "build": "babel src --out-dir build --source-maps",
    "e": "node example/run",
    "example/message.js": "yarn e example/message",
    "example/error.js": "node example/run example/error",
    "example/transparent.js": "node example/run example/transparent",
    "example/read-file.js": "node example/run example/read-file",
    "example/erotic.js": "node example/run example/erotic"
  },
  "files": [
    "build",
    "es5"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/erotic.git"
  },
  "keywords": [
    "error",
    "stack"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/erotic/issues"
  },
  "homepage": "https://github.com/artdecocode/erotic#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.49",
    "@babel/register": "7.0.0-beta.49",
    "babel-plugin-transform-rename-import": "2.2.0",
    "eslint": "4.19.1",
    "eslint-config-artdeco": "1.0.0",
    "noddy": "1.1.1",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  },
  "dependencies": {
    "clean-stack": "1.3.0"
  }
}
