{
  "name": "erotic",
  "version": "0.2.0",
  "description": "Capture synchronous part of the error stack in asynchronous functions",
  "main": "src/index.js",
  "scripts": {
    "test": "zoroaster test/spec",
    "test-es5": "zoroaster es5/test/spec",
    "test-watch": "zoroaster test/spec --watch",
    "debug": "ZOROASTER_TIMEOUT=99999 node --inspect=5858 node_modules/.bin/zoroaster test/spec/ --watch",
    "debug-es5": "ZOROASTER_TIMEOUT=99999 node --debug=5858 node_modules/.bin/zoroaster es5/test/spec/ --watch",
    "build-src": "babel src --out-dir es5/src",
    "build-test": "babel test --out-dir es5/test",
    "build": "run-s build-src build-test"
  },
  "files": [
    "src",
    "es5/src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/erotic.git"
  },
  "keywords": [
    "error",
    "stack"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/erotic/issues"
  },
  "homepage": "https://github.com/artdecocode/erotic#readme",
  "devDependencies": {
    "zoroaster": "1.1.0"
  },
  "dependencies": {
    "clean-stack": "1.3.0"
  }
}
